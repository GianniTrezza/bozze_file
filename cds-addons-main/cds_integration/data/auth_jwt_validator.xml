<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="cds_services_validator" model="auth.jwt.validator">
            <field name="name">cds</field>
            <field name="audience">auth_jwt_cds_services</field>
            <field name="issuer">cds issuer</field>
            <field name="signature_type">secret</field>
            <field name="secret_algorithm">HS256</field>
            <field name="secret_key">secret_key</field>
            <field name="user_id_strategy">static</field>
            <field name="static_user_id">1</field>
        </record>
        
    </data>
</odoo>
