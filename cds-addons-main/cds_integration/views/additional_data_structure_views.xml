    <data>

        <!-- ********************** MODELLO cds.transition.path ********************** -->
        <!-- form view -->
        <record id="cds_transition_path_form_view" model="ir.ui.view">
            <field name="name">cds.transition.path.form.view</field>
            <field name="model">cds.transition.path</field>
            <field name="arch" type="xml">
                <form string="Cds transition path form">
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                            </group>
                        </group>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
                </form>
            </field>
        </record>

        <!-- tree view -->
        <record id="cds_transition_path_tree_view" model="ir.ui.view">
            <field name="name">cds.transition.path.tree.view</field>
            <field name="model">cds.transition.path</field>
            <field name="arch" type="xml">
                <tree string="Cds transition path tree">
                    <field name="name"/>
                </tree>
            </field>
        </record>


